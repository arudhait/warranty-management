<!doctype html>
@*<html lang="en" data-layout="vertical" data-layout-style="default" data-sidebar="dark" data-topbar="dark" data-sidebar-size="sm" data-layout-width="fluid" data-layout-position="fixed" data-sidebar-visibility="show" style="min-height: calc(100vh - 1px);" data-bs-theme="dark">*@
<html lang="en" data-layout="vertical" data-topbar="light" data-layout-style="default" data-sidebar="dark" data-sidebar-size="sm" data-sidebar-image="none" data-layout-mode="dark" data-layout-width="fluid" data-body-image="img-3" data-sidebar-visibility="show" style="min-height: calc(100vh - 1px);">

<head>
    @await Html.PartialAsync("_Meta")
    @RenderSection("styles", required: false)
    @await Html.PartialAsync("_Css")
</head>
<body>
    <div class="preloader" style="display:none;margin: 0 auto;position: absolute;z-index: 9998;height: 100%;width: 100%;opacity: 0.5;">
        <div class="spinner-grow" style="width: 5rem; height: 5rem;position: absolute;left: 50%;top: 50%;" role="status">
            <span class="sr-only">Loading...</span>
        </div>
    </div>
    <!-- Begin page -->
    <div id="layout-wrapper">
        @await Html.PartialAsync("_topbar")
        @await Component.InvokeAsync("Menu")
        <!-- ============================================================== -->
        <!-- Start right Content here -->
        <!-- ============================================================== -->
        <div class="main-content">
            <div class="page-content">
                <div class="container-fluid">
                    @await Html.PartialAsync("_PageTitle")
                    @RenderBody()
                </div>
            </div>
            <div id="common-lg-dialog" class="modal fade" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" data-backdrop="false">
                <div class="modal-dialog modal-lg">
                    <div class="modal-content" id="common-lg-DialogContent">
                    </div>
                    <!-- /.modal-content -->
                </div>
                <!-- /.modal-dialog -->
            </div>
            <div id="common-md-dialog" class="modal fade" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" data-backdrop="false">
                <div class="modal-dialog modal-md">
                    <div class="modal-content" id="common-md-DialogContent">
                    </div>
                    <!-- /.modal-content -->
                </div>
                <!-- /.modal-dialog -->
            </div>
            <div id="common-lg-dialog" class="modal fade" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" data-backdrop="false">
                <div class="modal-dialog modal-lg">
                    <div class="modal-content" id="common-lg-DialogContent">
                    </div>
                    <!-- /.modal-content -->
                </div>
                <!-- /.modal-dialog -->
            </div>
            <div id="fullscreeexampleModal" class="modal fade" role="dialog" aria-labelledby="fullscreeexampleModalLabel" aria-hidden="true" data-backdrop="false">
                <div class="modal-dialog modal-fullscreen">
                    <div class="modal-content" id="fullscreeexampleModal-DialogContent">
                    </div>
                    <!-- /.modal-content -->
                </div>
                <!-- /.modal-dialog -->
            </div>
            <div id="common-dialog" class="modal fade" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" data-backdrop="false">
                <div class="modal-dialog">
                    <div class="modal-content" id="common-DialogContent">
                    </div>
                </div>
                <!-- /.modal-dialog -->
            </div>
            <div id="common-xxl-dialog" class="modal fade" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" data-backdrop="false">
                <div class="modal-dialog modal-xl" style="  max-width: 90% !important;">
                    <div class="modal-content" id="common-xxl-DialogContent">
                    </div>
                    <!-- /.modal-content -->
                </div>
                <!-- /.modal-dialog -->
            </div>
            @await Html.PartialAsync("_Footer")
        </div>
    </div>
    <!-- END layout-wrapper -->
    @*@Html.PartialAsync("_customizer")*@



    @await Html.PartialAsync("_Script")
    @RenderSection("scripts", required: false)
</body>
</html>
