@*<!-- start page title -->
<div class="row">
    <div class="col-12">
        <div class="page-title-box d-sm-flex align-items-center justify-content-between">
            <h4 class="mb-sm-0">@Html.Raw(ViewBag.PageName)</h4>
            @if (ViewBag.ParentPageName != null && ViewBag.PageName != null)
            {
                <div class="page-title-right">
                    <ol class="breadcrumb m-0">
                        <li class="breadcrumb-item"><a href="@Url.Action("Index", "Lead")">@Html.Raw(ViewBag.ParentPageName)</a></li>
                        <li class="breadcrumb-item active">@Html.Raw(ViewBag.PageName)</li>
                    </ol>
                </div>
            }
        </div>
    </div>
</div>
<!-- end page title -->*@